<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.haufe.application.ReviewController">
   <children>
      <TextArea fx:id="textAreaReview" layoutX="14.0" layoutY="14.0" prefHeight="207.0" prefWidth="566.0" promptText="write your review for the movie!" />
      <Slider fx:id="sliderRating" layoutX="14.0" layoutY="235.0" majorTickUnit="10.0" max="50.0" prefHeight="14.0" prefWidth="566.0" showTickLabels="true" showTickMarks="true" />
      <Text layoutX="14.0" layoutY="277.0" strokeType="OUTSIDE" strokeWidth="0.0" text="rating" />
      <Button fx:id="buttonSend" layoutX="497.0" layoutY="350.0" mnemonicParsing="false" onAction="#buttonPressedSend" text="send" />
      <Button fx:id="buttonReturn" layoutX="30.0" layoutY="350.0" mnemonicParsing="false" onAction="#buttonPressedReturn" text="return" />
   </children>
</AnchorPane>
