<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.haufe.application.RecommendController">
   <children>
      <ListView fx:id="listViewGroups" layoutX="14.0" layoutY="14.0" prefHeight="374.0" prefWidth="200.0" />
      <TextArea fx:id="textAreaRecommendation" layoutX="237.0" layoutY="22.0" prefHeight="206.0" prefWidth="346.0" promptText="write your recommendation!" />
      <Button fx:id="buttonReturn" layoutX="262.0" layoutY="340.0" mnemonicParsing="false" onAction="#buttonPressedReturn" text="return" />
      <Button fx:id="buttonSend" layoutX="500.0" layoutY="340.0" mnemonicParsing="false" onAction="#buttonPressedSend" text="send" />
   </children>
</AnchorPane>
